{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-course-hemanth"
		},
		"linkserviceDL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkserviceDL'"
		},
		"linked_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"linkserviceDL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedatahemanth.dfs.core.windows.net/"
		},
		"managrTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/acf1a358-b99c-4f1f-b79b-9597fdf87f6a/resourceGroups/RG-ADFCourse/providers/Microsoft.Storage/storageAccounts/storagedatahemanth"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-course-hemanth_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkserviceDL_accountKey": {
						"value": "[parameters('linkserviceDL_accountKey')]"
					},
					"linked_git_properties_typeProperties_url": {
						"value": "[parameters('linked_git_properties_typeProperties_url')]"
					},
					"linkserviceDL_properties_typeProperties_url": {
						"value": "[parameters('linkserviceDL_properties_typeProperties_url')]"
					},
					"managrTrigger_properties_typeProperties_scope": {
						"value": "[parameters('managrTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}